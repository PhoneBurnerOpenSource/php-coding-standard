{
  "name": "phoneburner/coding-standard",
  "description": "PhoneBurner PHP Coding Standards",
  "type": "phpcodesniffer-standard",
  "license": "MIT",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "PhoneBurner\\CodingStandard\\": "PhoneBurner/"
    }
  },
  "require": {
    "php": "^8.3 || ^8.4",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "slevomat/coding-standard": "^8.15",
    "squizlabs/php_codesniffer": "^3.10"
  },
  "extra": {
  }
}
